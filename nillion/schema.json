{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Store Timestamp and Data Array (Encrypted)",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "timestamp": {
          "type": "object",
          "properties": {
            "$share": {
              "type": "string"
            }
          },
          "required": ["$share"]
        },
        "data": {
          "type": "object",
          "properties": {
            "$share": {
              "type": "string"
            }
          },
          "required": ["$share"]
        }
      },
      "required": ["timestamp", "data"]
    }
  }
  